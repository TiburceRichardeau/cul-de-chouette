<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pierral.cdc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:icon="@drawable/cdc_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pierral.cdc.CulDeChouette"
            android:label="@string/title_activity_cul_de_chouette"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pierral.cdc.CDCGame"
            android:label="@string/title_activity_cul_de_chouette"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pierral.cdc.SettingsActivity"
            android:label="@string/title_activity_cul_de_chouette"
            android:screenOrientation="portrait" />
    </application>

</manifest>